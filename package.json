{
  "name": "@nebulaai/core",
  "version": "1.0.1",
  "description": "Core utilities and types for NebulaAI toolkit",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nebulaai",
    "core",
    "types",
    "utilities",
    "startup",
    "toolkit"
  ],
  "author": "NebulaAI Team <support@temporalaitechnologies.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nebulaaitech/core.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest --passWithNoTests",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.4.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12"
  },
  "dependencies": {
    "@types/node": "^20.11.30"
  },
  "bugs": {
    "url": "https://github.com/nebulaaitech/core/issues"
  },
  "homepage": "https://github.com/nebulaaitech/core#readme"
}